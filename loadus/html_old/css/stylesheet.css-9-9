.msgList li .application {max-width: 100%;width: 123px; text-align:right;}
.msgList li .application a{ font-size:15px; color:#050eff; font-weight:500;}
.msgList li .application a:hover{ text-decoration:underline;}
.msgList li .application span{ display:block; color:#424242; padding-top:4px;}

/*============== Dashboard Message detail==================*/
.userApp{padding:20px 20px; position: relative;}
.userApp:before{content: ''; border-top:2px solid #ececec; width: 100vw;bottom:0; position: absolute; left: -30px;}
.userApp .back{ float:right; color:#646464; font-size:18px; text-transform:uppercase; font-weight:500;}
.userApp .back:hover{ color:#050eff;}
.userImage{ display:table;}
.userImage h5{ font-size:18px; color:#2d3845; padding-bottom: 0;}
.userImage p{font-side:15px;}
.userImage p { font-size: 16px;}
.userImage p a{ display:inline-block; color:#050eff; font-size: 15px; font-weight: 500;}
.userImage figure, .userImage figure img{ border-radius:50%;}
.userImage figure{ display:table-cell; width:90px;}
.chatList{margin:0px; padding:20px 20px 80px;}
.chatList ul{margin:0px; padding:0px;}
.chatList li{padding:10px 10px 10px 50px; position:relative;}
.chatList p{margin-top:10px; margin-bottom: 0px;}
.chatList span{font-size:16px; background:#f5f5f5; color:#38424e; font-weight:400; display:inline-block; line-height:20px; padding:9px 15px; border-radius:5px;}
.chatList li.plumberChat{}
.chatList li.plumberChat figure{position:absolute; top:15px; left:0px; height:35px; width:35px; border-radius:100%; overflow:hidden;}
.chatList li.plumberChat figure img{height:35px; width:100%; object-fit:cover;}
.chatList li.customerChat{text-align:right;}
.chatList li.customerChat span{background:#050eff; color:#fff;}
.BackBtn {float: right;background:#424f5d;color:#fff !important;border-radius: 5px;padding: 6px 20px;font-size: 13px !important;text-transform: uppercase;}
.BackBtn:hover{background: #ffe100; color: #000 !important;}
.date2 {font-weight: 500;text-align: center;border-bottom: 1px solid #ededed;padding-bottom: 10px;}
.chatBox{ border-top:1px solid #ccc;}
.chatBox input{ height:60px; padding:0 19px; color:#38424e; font-size:14px; width:93%;}
.chatBox input:placeholder{color:#fe0000;}
.chatBox button{ background:#2d3845; height:40px; width:40px; line-height:40px; text-align:center; border-radius:50%; border: none;}
.chatBox button:hover{ background:#050eff;}
.chatBox button img{ vertical-align: middle;}
.custom-bar {width: 100%; height: 100vh; max-width: 100%;}

/*-----------notifications ---------*/ 
.notiMsg{ clear: both;}
.notiMsg li{ font-size: 18px; color: #4f4f4f; font-weight: 400; padding: 10px 0; line-height: 40px;}
.notiMsg li + li{border-top:1px solid rgba(0,0,0,.1);}
.notiMsg li:last-child{border-bottom:1px solid rgba(0,0,0,.1);}
/*-----------Create Group ---------*/
.loginForm textarea{border: none; border-radius: 0px; padding: 0px; margin: 0px; height: 80px; font-size: 18px; font-weight: 500;}
.upload-btn-wrapper {position: relative;overflow: hidden;display: inline-block;}
.upload-btn-wrapper .btn {border: none;color: gray;background: none;padding: 0;border-radius: 0;font-size:18px;font-weight: normal;width: auto;text-transform: capitalize;text-align: left; line-height: inherit; height: auto;}
.upload-btn-wrapper input[type=file] {font-size: 100px;position: absolute;left: 0;  top: 0;opacity: 0;}
.loginForm .stePrice{color: #252525; font-size: 18px; font-weight:500;}
.loginForm .sendInv button{font-size: 16px; width: 180px; height: 44px; line-height: 44px;}

.loginForm select{border: none; padding: 0px;-moz-appearance: none;-webkit-appearance: none; margin: 0px;}
.loginForm select option{padding: 0px;}

.totalMember{ display: inline-block; width: 250px; vertical-align: top;}
.totalMember h4{color: #252525; font-size: 18px; font-weight:500; padding-bottom: 10px;}
.totalMember li{ margin-bottom: 7px; color: #4f4f4f; font-size: 16px;}
.totalMember li a{ color: #050eff; font-size: 18px;}

body.modal-open .modalIn .modal-dialog{float:none;}
body.modal-open .modalIn .modal-content{padding:0;margin-top:0;}
body.modal-open .modalIn .modal-content{border:none;box-shadow:none; border-radius: 8px;}
.modalIn .modal-body{padding:0;}
.mdTitle{border-radius:13px 13px 0 0;background:#fff;padding:20px 85px 12px 46px;}
.mdTitle h2{ font-size: 32px; display: block;font-weight: 500;}
.mdTitle h2 span{float: right; padding-top: 10px; font-size: 16px;}
.mdkData{padding:20px 0;}
.mdkData li{padding:15px 30px;clear:both;color:#212121;font-size:18px;}
.mdkData li span{display:block;color:#616161;}
.mdkData li ~ li{border-top:1px solid #aeaeae;}
.modalIn button.close{opacity:1;text-shadow:none;right: -50px;top:0; position: absolute; z-index: 1;}
.modalIn .modal-body::after {background: #0209ff;width:40px;height: 100%;content: "";position: absolute;right:0;top:0;border-radius:0px 8px 8px 0;}
.inviteUser{padding: 45px 85px 0px 45px;}

/* ====================== Responsive =======================================*/
@media (min-width:768px){
.modal-dialog {max-width: 650px;}
.modalImage	.modal-dialog{max-width: 1100px;}
.modalImage .modal-body img{border-radius: 25px;}	
}
@media (min-width:1500px) {
/*.slider .owl-carousel .owl-item img{width:100%;max-width:100%;height:500px; object-fit: contain;}*/
	.slider {position: relative;height: 500px;overflow: hidden;}
	.slider .container{top:26%;}
	
}

@media (max-width:991px) {
	.cstm label{ font-size: 17px;}
	.tierTp{justify-content: flex-start !important;margin-bottom:20px;}
	.sideMenu{background: none;}
	.dshContent {padding: 40px 30px 25px 30px;}
	.modalIn button.close {right: 2px;top: 15px;}
}
@media (max-width:767px) {

	.space50 {padding: 20px 24px;}
	.loginForm {padding: 45px 70px 45px 20px;}
	.loginForm p, .loginForm p a{font-size: 15px;}
	.loginForm p a{padding-top: 15px; display: inline-block;}
	.cstm label{font-size: 15px;}
    .tierTp{display:block !important;}
    .tiers h2.title{margin-top:30px;}
    .tiersFig img{display:none;}
    .tiersFig .tImg img{display:block;}
    .tImg {position: static !important; display:inline-block;vertical-align:top; margin:0px 10px;}
	.tImg .imgIn{display: block;
    float: right;position:relative;top:-20px;}
    .msgLink{margin:0 15px;width:58px;height:58px;line-height:58px;float:right;}
    h2.title span{display:block;margin-top:5px;float: none;}
    .joinBtn{margin:15px 0;}
	.totalMember{width: 150px;}
}
@media (max-width:599px) {
	.dshContent {padding: 20px 30px 25px 20px;}
	.mdTitle{padding: 20px 63px 12px 21px}
}