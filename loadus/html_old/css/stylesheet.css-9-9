		/*
  	Flaticon icon font: Flaticon
  	Creation date: 20/07/2020 21:38
  	*/

@font-face {
	font-family: "Flaticon";
	src: url("../fonts/Flaticon.eot");
	src: url("../fonts/Flaticon.eot?#iefix") format("embedded-opentype"),
		 url("../fonts/Flaticon.woff2") format("woff2"),
		 url("../fonts/Flaticon.woff") format("woff"),
		 url("../fonts/Flaticon.ttf") format("truetype"),
		 url("../fonts/Flaticon.svg#Flaticon") format("svg");
	font-weight: normal;
	font-style: normal;
  }
  
  @media screen and (-webkit-min-device-pixel-ratio:0) {
	@font-face {
	  font-family: "Flaticon";
	  src: url("../fonts/Flaticon.svg#Flaticon") format("svg");
	}
  }
  
  [class^="flaticon-"]:before, [class*=" flaticon-"]:before,
  [class^="flaticon-"]:after, [class*=" flaticon-"]:after {   
	font-family: Flaticon;
  font-style: normal;
  line-height: 1;
  
  }
  
  .flaticon-drop:before { content: "\f100"; }
  .flaticon-key:before { content: "\f101"; }
  .flaticon-agreement:before { content: "\f102"; }
  .flaticon-bar-chart:before { content: "\f103"; }
  .flaticon-marker:before { content: "\f104"; }
  .flaticon-mail:before { content: "\f105"; }
.flaticon-chat:before { content: "\f100"; }


::-webkit-input-placeholder{opacity:1; color:#b6b6b6; font-family: 'Poppins', sans-serif; font-size:15px;}
:-moz-placeholder{opacity:1; color:#b6b6b6; font-family: 'Poppins', sans-serif; font-size:15px;}
::-moz-placeholder{opacity:1; color:#b6b6b6; font-family: 'Poppins', sans-serif; font-size:15px;}
:-ms-input-placeholder{opacity:1; color:#b6b6b6; font-family: 'Poppins', sans-serif; font-size:15px;}
*,*:after,*:before{box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; -ms-text-size-adjust: 100%; -webkit-text-size-adjust: 100%; font-stretch:normal; text-rendering: opximizeLegibility;-webkit-font-smoothing: antialiased;-moz-osx-font-smoothing: grayscale;}
:focus{outline:0 none;}
html{font-size:100%; -webkit-text-size-adjust:100%; -ms-text-size-adjust:100%;}
body{margin:0; -webkit-font-smoothing:antialiased; -webkit-text-size-adjust:100%; font-family:'Rubik', sans-serif;}
img{border: 0 none; max-width: 100%; vertical-align: top;}
a{color:#383939; font-family:'Rubik', sans-serif; text-decoration: none; transition:all ease 0.3s; -o-transition:all ease 0.3s; -moz-transition:all ease 0.3s; -ms-transition:all ease 0.3s; -webkit-transition:all ease 0.3s; cursor: pointer;}
a:hover{color:#2586ff; text-decoration:none;}
p{margin:0 0 15px; font-size:14px; color:#525252; line-height:22px; font-family:'Rubik', sans-serif; font-weight: normal;}
p:last-child, p:last-of-type, p:only-child, p:only-of-type{margin-bottom: 0;}
nav ul,nav ol,li{list-style:none;}
ul{padding:0 0; margin:0 0;}
img, iframe{border:0; -ms-interpolation-mode:bicubic; max-width:100%;}
form{margin:0;}
button,input,select,textarea{font-size:100%; margin:0; vertical-align:baseline; vertical-align:middle;}
select{font-size:100%; background-color:#fff; height: 45px; margin-bottom: 20px; width: 100%; vertical-align:baseline; vertical-align:middle; border: 2px solid #ebebeb;  border-radius: 0px;}
input, textarea { padding: 15px 17px; box-sizing: border-box; line-height: 1.2; width: 100%; background-image: none; background-color:#fcfafa; margin-bottom: 10px; height: auto;  border: none ; border-radius: 0px; }
input:focus, textarea:focus,.form-control:focus{box-shadow:none; }
button::-moz-focus-inner,input::-moz-focus-inner{border:0; padding:0;}
textarea{overflow:auto; vertical-align:top; height:160px; resize:none;}
table{border-collapse:collapse; border-spacing:0;}
address{font-style: normal; font-size: 14px;}
.text-white, .text-white *{color:#fff !important;}
.btn, input[type="submit"].btn{font-size:14px; color:#fff; font-family:'Roboto', sans-serif; font-weight: normal; line-height:1;display:inline-block; padding:10px 30px; background-color:#0209ff; background-image: none;border-radius:6px;}
.btn:hover{color:#fff; box-shadow:2px 2px 3px 0px rgba(0, 0, 0, 0.5); -webkit-box-shadow:2px 2px 3px 0px rgba(0, 0, 0, 0.5); -o-box-shadow:2px 2px 3px 0px rgba(0, 0, 0, 0.5); -moz-box-shadow:2px 2px 3px 0px rgba(0, 0, 0, 0.5); transition:all ease 0.3s; -o-transition:all ease 0.3s; -moz-transition:all ease 0.3s; -ms-transition:all ease 0.3s; -webkit-transition:all ease 0.3s;}
h1,h2,h3,h4,h5,h6{font-weight: normal; font-family:'Rubik', sans-serif;  color:#525252; margin-bottom:0;} 
h3{ font-size:24px; font-weight: 500; line-height: 30px; text-transform:capitalize; padding-bottom:10px; margin-bottom:0;}
h4{font-size:18px; line-height: 24px; text-transform:capitalize; padding-bottom:15px; margin-bottom:0;}
h5{font-size:16px; line-height: 22px; font-weight: 500; line-height:1.2; text-transform:capitalize; padding-bottom:15px; margin-bottom:0;}
h6{font-size:14px; line-height: 20px; font-weight: 500; line-height:1.2; text-transform:capitalize; padding-bottom:15px; margin-bottom:0;}
h2.title{font-size: 40px; font-weight: 500; font-family:'Rubik', sans-serif;  color:#434343; line-height:1; padding:32px 0 5px 30px;  position: relative; background-image: url('../img/title-bg.png'); background-repeat: no-repeat; display: inline-block;}
h2.title:before{content: ' ';position: absolute; left: 0px; top: 0px; height: 98%; width: 6px; background-color: transparent; border-style: solid;  border-width: 0px 2px;}
h2.title.blue{background-position:-70px 0 ;}
h2.title.white{background-position:30px -60px; color: #fff;}
h2.title.blue:before{border-color: #050fff;}
h2.title.white:before{border-color: #fff;}
.section_title h2 span{color: #454545;}
.section_title h3{font-size: 24px; font-weight: 500; font-family:'Rubik', sans-serif;  color:#525252; text-transform: uppercase; line-height:1; padding-bottom:15px; margin-bottom: 10px; position: relative;}
section:not(.slider):not(.page-banner){padding: 55px 0 70px;}
/*header  */

.sticky-header.header_fixed{position:fixed;top:0;left:0;width:100%;max-width:100%;z-index:99;box-shadow:0 8px 6px -6px rgba(0,0,0,.4); background-color: rgba(255,255,255,.9);}
.animated{animation-duration:1s;animation-fill-mode:both;}
@keyframes fadeInDown{from{opacity:0;transform:translate3d(0,-100%,0);}
to{opacity:1;transform:none;}
;}.fadeInDown{animation-name:fadeInDown;}
.logo a{display: inline-block; padding-top: 3px;}
	/* --> menu */
	.wsmenu {font-family: 'Rubik', sans-serif;}
.wsmenu-submenu{min-width: 180px; top: 99%;}

/*slider*/
.slider .owl-carousel .owl-item img{width:100%;max-width:100%;/*height:auto; max-height: 570px;*/}
.slide-content {width: 760px; padding-left: 31px;}
.slide-content h1{font-size: 51px; font-weight: 500; color:#fff; line-height:62px; padding-bottom:10px; margin-bottom: 0; position: relative;}
.slide-content h1:before{content: ' ';position: absolute; left: -31px; top: 13px; height: 75%; width: 6px; background-color: transparent; border: solid #fff; border-width: 0px 2px;}
.slide-content p{font-size: 20px; font-weight: normal; color:#fff; line-height:29px; padding-bottom:20px; margin-bottom: 0; width: 530px;}
.slider{position: relative;}
.slider .container{position: absolute;top: 45%; left: 50%; transform: translate(-50%, -45%); -webkit-transform: translate(-50%, -45%); -moz-transform: translate(-50%, -45%); -o-transform: translate(-50%, -45%);}
.owl-dots{text-align: center; margin-top: 40px;}
.owl-carousel button.owl-dot {width: 45px; height: 7px; background-color: #d3d3d3; border-radius: 7px; margin: 0 3px;}
.owl-carousel button.owl-dot.active{background-color: #0209ff; outline: none;}
/*available group*/
section.section.avail-group{padding: 55px 0 68px;}
.avail-group .section-title{padding-bottom: 50px;}
.group-content{padding: 20px 20px 25px; border-bottom: 1px solid #d3d3d3;}
.join-btn{text-align: center; padding: 16px 15px}
.group-carousel .item {margin: 4px 4px 4px; box-shadow: 0px 0px 4px 2px rgba(0, 0, 0, 0.2);}

/*creat an group*/
section.section.team-group{background-image: url('../img/group-bg.jpg'); background-repeat: no-repeat; background-position: center center; padding: 97px 0 92px;}
.team-img{position: relative; padding: 0 0 59px 56px; display: inline-block;}
.team-img img{position: relative; z-index: 1;}
.team-img:after{content: ''; position: absolute; height: 100%;height: calc(100% - 56px); width: 100%; width: calc(100% - 59px); background-color: #0209ff; left: 0px; bottom: 0px;}
.team-group-content p{font-size: 18px; color:#fff; line-height:32px; font-weight: 300;}
.option-list{padding-top: 50px;}
.option-list li{color:#fff; font-size: 18px; font-weight: bold; line-height: 24px; background-color:#555555; position: relative; width: 47%; width: calc(50% - 15px); padding: 20px 20px 20px 85px; font-family:'Roboto'; font-weight: bold;}
.option-list li:hover{background-color: #0209ff; transition:all ease 0.3s; -o-transition:all ease 0.3s; -moz-transition:all ease 0.3s; -ms-transition:all ease 0.3s; -webkit-transition:all ease 0.3s; cursor: pointer;}
.team-group-content .section-title {padding-bottom: 35px;}
.option-list li span{color: #fff; line-height: 1; position: absolute; font-size: 40px; top:50%; left: 21px; transform: translateY(-50%); -webkit-transform: translateY(-50%); -moz-transform: translateY(-50%); -o-transform: translateY(-50%);}
.option-list li:nth-child(odd){margin-right: 30px;}
.option-list li:nth-child(n+3){margin-top: 20px;}
/* member subscription*/
section.section.member {padding: 60px 0 70px;}
.member .section-title{text-align: center; padding-bottom: 68px;}
.plan-box {width: 31.5%; background-color: #f2f2f2; padding: 37px 25px 37px 35px; border-radius: 25px;box-shadow: 0 0 5px 3px rgba(214, 205, 250, 0.6);}
.plan-box:not(:first-child) {margin-left: 110px;}
.plan-type{text-align: center;}
.plan-type h3{font-size: 28px; font-weight: normal; color:#252525; line-height:32px; font-weight: 300; padding-bottom: 20px; position: relative;}
